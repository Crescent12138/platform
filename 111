create table predict_corpus.person_information
(
    id                               int auto_increment
        primary key,
    name                             varchar(64)          null comment '姓名',
    age                              int                  null comment '年龄',
    ctime                            timestamp            null,
    utime                            timestamp            null,
    phone                            varchar(20)          null comment '电话',
    email                            varchar(32)          null,
    birthplace                       varchar(64)          null,
    is_agree                         tinyint(1) default 0 not null,
    sex                              tinyint(1)           null,
    country                          varchar(32)          null,
    order_language_list              varchar(328)         null comment 'list[string]',
    learned_language_list            varchar(328)         null,
    map_read_language_percent        varchar(648)         null,
    map_interaction_language_percent varchar(648)         null,
    map_cultural_identity_percent    varchar(328)         null comment 'map{string,int}',
    education_time                   int                  null,
    academic_credentials             varchar(64)          null,
    come_to_china                    timestamp            null,
    disability_list                  varchar(64)          null comment 'list[string]',
    chinese_number                   varchar(328)         null,
    learning_chinese_age             int                  null,
    learn_chinese_time               int                  null comment '学习时长',
    oral_age                         int                  null,
    talk_well_age                    int                  null,
    learning_read_age                int                  null,
    read_well_age                    int                  null,
    chinese_country                  int                  null comment 'month',
    chinese_family                   int                  null,
    chinese_school                   int                  null,
    chinese_oral                     int                  null,
    understand_chinese_oral          int                  null,
    read_chinese                     int                  null,
    promote_friend_interaction       int                  null,
    promote_family_interaction       int                  null,
    promote_app_self_learning        int                  null,
    promote_watch_tv                 int                  null,
    promote_listen_radio             int                  null,
    frequency_friend_interaction     int                  null,
    frequency_family_interaction     int                  null,
    frequency_app_self_learning      int                  null,
    frequency_watch_tv               int                  null,
    frequency_listen_radio           int                  null,
    oral_accent                      int                  null,
    others_feeling_oral_accent       int                  null
)
    comment '个人信息';

